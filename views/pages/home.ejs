<h1>This is Ticket Viewer Homepage</h1>
<h2>Here are all tickets for your account</h2>

<ul>
    <% for (i=0; i < tickets.length; i++) { %>

        <li class="ticketId">
            <b>Ticket ID:</b>
            <%= tickets[i].id %>
                <b>Ticket status:</b>
                <%= tickets[i].status %>
                    <div class="ticket-card">

                        <%= JSON.stringify(tickets[i]) %>
                    </div>
        </li>






        <% } %>
</ul>
<script type="text/javascript" src="/static/home.js"></script>